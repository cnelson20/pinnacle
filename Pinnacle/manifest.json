{
    "name" : "Pinnacle",
    "description" : "Extension for live annotating webpages",
    "version" : "0.1",
    "manifest_version" : 3,
    "background" : {
        "server_worker" : "background.js"
    },
    "action" : {
        "default_popup" : "popup.html"
    },
    "icons" : {
        "16": "/images/pin16.png",
        "32": "/images/pin32.png",
        "48": "/images/pin48.png",
        "128": "/images/pin128.png"
    },
    "options_page": "options.html",
    "permissions": ["storage", "activeTab", "scripting", "tabs"]
}